@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>代理申请 - 微商系统演示版</title>
    <meta http-equiv="X-UA-Compatible" content="IE=7,IE=9" />
    <meta name='viewport' content='width=640px, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no,target-densitydpi=device-dpi' />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <meta name="viewport" content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" />
    <meta name="format-detection" content="telephone=no" />
    <link href="/css/jlpmain.css?20161222" rel="stylesheet" type="text/css" />
    <script src="~/js/jquery-1.10.2.min.js"></script>
    <script src="~/js/Common/layer.mobile-v2.0/layer_mobile/layer.js"></script>
    <style type="text/css">
        .centent_total .sqdls_up table.t1 {
            margin-bottom: 20px;
        }

        .centent_total .sqdls_up table.t22 tr td.td1 {
            width: 30%;
            height: 45px;
            line-height: 40px;
            float: left;
            color: #009b8d;
            font-size: 15px;
            font-weight: bold;
            letter-spacing: 1px;
        }

        .centent_total .sqdls_up table.t22 tr td.td2 {
            width: 70%;
            float: right;
        }

            .centent_total .sqdls_up table.t22 tr td.td2 input, .centent_total .sqdls_up table.t22 tr td select {
                width: 100%;
                text-indent: 5px;
                float: left;
                height: 35px;
                color: #333;
                font-size: 12px;
                font-family: Microsoft YaHei;
                font-weight: normal;
                background-color: #b2e3df;
                border-radius: 0;
                -moz-border-radius: 0;
                -webkit-border-radius: 0;
                border: 1px solid #18a598;
            }

        .centent_total .sqdls_up table.t22 tr td select {
            width: 100%;
            -webkit-appearance: none;
            background-image: url(/images/88.png);
            background-repeat: no-repeat;
            background-size: 14% 104%;
            background-position: right;
            background-origin: content-box;
        }








    </style>




    <link rel="stylesheet" type="text/css" href="/js/Common/danmu/static/css/bootstrap.min.css" media="screen" />
    <link rel="stylesheet" type="text/css" href="/js/Common/danmu/css/zzsc-demo.css">
    <link rel="stylesheet" type="text/css" href="/js/Common/danmu/static/css/style.css" />
    <link rel="stylesheet" type="text/css" href="/js/Common/danmu/dist/css/barrager.css">
    <link rel="stylesheet" type="text/css" href="/js/Common/danmu/static/pick-a-color/css/pick-a-color-1.2.3.min.css">
    <link type="text/css" rel="stylesheet" href="/js/Common/danmu/static/syntaxhighlighter/styles/shCoreDefault.css" />

</head>
<body>

    <form id="form1" data-ajax="false">
        <div id="page1" data-role="page">
            <div data-role="content" id="content">
                <div class="centent_total">
                    <div class="sqdls_up">
                        <table class="t1">
                            <tr>
                                <td class="d1">
                                    <span>上级微信：</span>
                                </td>
                                <td class="d2">
                                    <input name="ChiefWx" placeholder="输入上级微信" />
                                </td>
                            </tr>
                            <tr>
                                <td class="d1">
                                    <span>上级名称：</span>
                                </td>
                                <td class="d2">
                                    <input name="ChiefName" placeholder="输入上级名称" />
                                </td>
                            </tr>
                            <tr>
                                <td class="d1">
                                    <span>上级级别：</span>
                                </td>
                                <td class="d2">
                                    <input name="ChiefType" placeholder="输入上级级别" />
                                </td>
                            </tr>
                        </table>
                        <table class="t22">
                            <tr>
                                <td></td>
                            </tr>
                            <tr>
                                <td class="td1">
                                    申请级别：
                                </td>
                                <td class="td2">

                                    @Html.DropDownList("ParentID", WeModels.SelectListHelper.GetC_UserTypeID(null), new { })
                                </td>
                            </tr>
                            <tr>
                                <td class="td1">
                                    姓名：
                                </td>
                                <td class="td2">
                                    <input name="Name" id="Text3" type="text" placeholder="请输入申请人全名（必填）" />
                                </td>
                            </tr>
                            <tr>
                                <td class="td1">
                                    微信号：
                                </td>
                                <td class="td2">
                                    <input id="Text4" name="wxNo" type="text" placeholder="请输入真实微信（必填）" />
                                </td>
                            </tr>
                            <tr>
                                <td class="td1">
                                    手机号：
                                </td>
                                <td class="td2">
                                    <input id="Text6" name="Phone" type="text" placeholder="请输入有效电话号码（必填）" />
                                </td>
                            </tr>
                            <tr>
                                <td class="td1">
                                    身份证：
                                </td>
                                <td class="td2">
                                    <input id="Text5" name="Card" type="text" placeholder="请输入正确有效的身份证（必填）" />
                                </td>
                            </tr>
                            <tr>
                                <td style="height: 10px;"></td>
                            </tr>
                            <tr>
                                <td class="rsx111">
                                    <input id="file_temporaryImage" hidden="hidden" type="file" name="file_temporaryImage" />
                                    <div id="setimg">

                                        <img id="idcardshow" src="/images/74.png" alt="" />
                                        <span>身份证正面照片（必填）</span>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td style="height: 20px;"></td>
                            </tr>
                            <tr class="r1">
                                <td style="text-align: center;">
                                    <button onclick="send()" type="button" style="background-color: #009b8d; color: white; padding: 10px; width: 80%;border-radius:5px;border: 0; border: none; ">提交</button>
                                </td>
                            </tr>
                            <tr>
                                <td></td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
            <div class="All_foot">
                <div class="foot">
                    <table>
                        <tr>
                            <td>
                                <a href="/">
                                    <div class="div1">
                                        <img src="/images/25.png" alt="" />
                                    </div>
                                    <div class="div2">
                                        返回首页
                                    </div>
                                </a>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </form>


    <textarea class="form-control" id="barrager-code" rows="14"></textarea>

</body>
</html>
<script type="text/javascript" src="/js/Common/danmu/static/js/jquery-1.9.1.min.js"></script>
<script type="text/javascript" src="/js/Common/danmu/static/js/bootstrap.min.js"></script>
<script type="text/javascript" src="/js/Common/danmu/static/js/tinycolor-0.9.15.min.js"></script>
<script type="text/javascript" src="/js/Common/danmu/dist/js/jquery.barrager.min.js"></script>
<script type="text/javascript" src="/js/Common/danmu/static/syntaxhighlighter/scripts/shCore.js"></script>
<script type="text/javascript" src="/js/Common/danmu/static/syntaxhighlighter/scripts/shBrushJScript.js"></script>
<script type="text/javascript" src="/js/Common/danmu/static/syntaxhighlighter/scripts/shBrushPhp.js"></script>
<script type="text/javascript" src="/js/Common/danmu/static/pick-a-color/js/pick-a-color-1.2.3.min.js"></script>  


<script>

    function run() {

        var item = {
            'info': "纠纷而几个人奇偶比交通环境",
            'speed': 6,
            'bottom': 20,
            'color': 'red',
            'old_ie_color': 'blue'



        };

        code = code.format(item);
        console.log(code);
        $('#barrager-code').val(code);
        eval(code);
    }

</script>