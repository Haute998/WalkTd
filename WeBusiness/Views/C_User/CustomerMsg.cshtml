@using WeModels
@{

    Layout = null;
}
@model C_User
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <title>代理信息</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <script src="~/js/layui-v1.0.3/layui/lay/dest/layui.all.js"></script>
    <link href="~/js/layui-v1.0.3/layui/css/layui.css" rel="stylesheet" media="all" />
    <script src="~/js/jquery-1.10.2.min.js"></script>
    <script src="~/js/jquery.tmpl.min.js"></script>
    <script src="~/js/common/Datehelper.js"></script>
</head>
<body>
    <fieldset class="layui-elem-field layui-field-title">
        <legend>代理信息</legend>
    </fieldset>
    <table class="layui-table" style="margin:20px;text-align:center">
        <tr>
            <td style="background-color: #e2e2e2">用户名</td>
            <td>@(Model.UserName)</td>
        </tr>
        <tr>
            <td style="background-color: #e2e2e2">姓名</td>
            <td>@(Model.Name)</td>
        </tr>
        <tr>
            <td style="background-color: #e2e2e2">手机号</td>
            <td>@(Model.Phone)</td>
        </tr>
        @*<tr>
            <td style="background-color: #e2e2e2">微信号</td>
            <td>@(Model.wxNo)</td>
        </tr>*@

        <tr>
            <td style="background-color: #e2e2e2">密码</td>
            <td>@(Model.PassWord)</td>
        </tr>
        <tr>
            <td style="background-color: #e2e2e2">所属地区</td>
            <td>@(Model.Province + Model.City + Model.Area+Model.WxQRCode)</td>
        </tr>
        <tr>
            <td style="background-color: #e2e2e2">添加时间</td>
            <td>@(Model.DatCreate)</td>
        </tr>
        @*<tr>

            <td style="background-color: #e2e2e2">审核时间</td>
            <td>@(Model.DatVerify)</td>
        </tr>*@
        @*<tr>
            <td colspan="2">
                <img style="max-width:500px;" src="@WeConfig.wx_domain@(Model.CardUrl)" />
            </td>
        </tr>*@

    </table>
</body>
</html>