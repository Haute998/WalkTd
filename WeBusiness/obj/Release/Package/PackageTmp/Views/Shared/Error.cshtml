@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>错误</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <script src="~/js/layui-v1.0.3/layui/lay/dest/layui.all.js"></script>
    <link href="~/js/layui-v1.0.3/layui/css/layui.css" rel="stylesheet" media="all" />
</head>
<body>
    <fieldset class="layui-elem-field layui-field-title">
        <legend>@(Model.Title??"出错啦")</legend>
    </fieldset>

    <blockquote class="layui-elem-quote">@(Model.Message??"出错啦")</blockquote>
</body>
</html>