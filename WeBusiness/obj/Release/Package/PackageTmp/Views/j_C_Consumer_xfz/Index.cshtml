@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="renderer" content="webkit|ie-comp|ie-stand" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" />
    <meta http-equiv="Cache-Control" content="no-siteapp" />
    <link rel="stylesheet" type="text/css" href="/layout/static/h-ui/css/H-ui.min.css" />
    <link rel="stylesheet" type="text/css" href="/layout/static/h-ui.admin/css/H-ui.admin.css" />
    <link rel="stylesheet" type="text/css" href="/layout/lib/Hui-iconfont/1.0.7/iconfont.css" />
    <link rel="stylesheet" type="text/css" href="/layout/lib/icheck/icheck.css" />
    <link rel="stylesheet" type="text/css" href="/layout/static/h-ui.admin/skin/default/skin.css"
          id="skin" />
    <link rel="stylesheet" type="text/css" href="/layout/static/h-ui.admin/css/style.css" />
    <title>消费者资料管理</title>
    <style type="text/css">
        .table td {
            text-align: center;
        }
    </style>
    <script src="~/js/jquery-1.10.2.min.js"></script>
    <script src="~/js/jquery.tmpl.min.js"></script>
    <script src="~/js/common/Datehelper.js"></script>
</head>
<body>

    <nav class="breadcrumb"><i class="Hui-iconfont">&#xe67f;</i> 首页 <span class="c-gray en">&gt;</span> 会员管理 <span class="c-gray en">&gt;</span> 消费者资料管理 <a class="btn btn-success radius r" style="line-height:1.6em;margin-top:3px" href="javascript:location.replace(location.href);" title="刷新"><i class="Hui-iconfont">&#xe68f;</i></a></nav>
    <div class="page-container">
        <form class="text-c" id="condition">
            <div class="text-c">
                关键字：
                <input type="text" name="keyword" id="" placeholder="账号/姓名" style="width: 250px"
                       class="input-text" />
                <button name="" id="search" class="btn btn-success" type="button">
                    <i class="Hui-iconfont">&#xe665;</i> 搜索
                </button>

            </div>
            <div class="mt-20">
                <table class="table table-border table-bordered table-hover table-bg dataTable" id="datahtml"></table>
                @Html.Partial("_Paging")
            </div>
        </form>
    </div>
    <script type="text/javascript" src="/layout/lib/layer/2.1/layer.js"></script>

    <script type="text/javascript" src="/layout/static/h-ui.admin/js/H-ui.admin.js"></script>
</body>
</html>
<script type="text/x-jquery-tmpl" id="datajs">
    <thead>
        <tr class="text-c">
            <th>
                账号
            </th>
            <th>
                头像
            </th>
            <th>
                姓名
            </th>
            <th>
                手机号
            </th>
            <th>
                地址
            </th>
            <th>
                性别
            </th>
            <th>
                可用积分
            </th>
            <th>
                已用积分
            </th>
        </tr>
    </thead>
    <tbody>
        {{each(i,ck) RtnList}}
        <tr class="text-c">
            <td>
                ${ck.UserName}
            </td>
            <td>
                <img src="${ck.wx_headurl}" width="50" height="50" />
            </td>
            <td>
                {{if ck.Name==""}}
                ${ck.wx_name}
                {{else}}
                ${ck.Name}
                {{/if}}
            </td>
            <td>
                ${ck.Mobile}
            </td>
            <td>
                {{if ck.Province+ck.City+ck.Area+ck.Address==""}}
                ${ck.wx_address}
                {{else}}
                ${ck.Province}${ck.City}${ck.Area}${ck.Address}
                {{/if}}
            </td>
            <td>
                {{if ck.Sex==""}}
                {{if ck.wx_sex==1}}
                男
                {{else}}
                女
                {{/if}}
                {{else}}
                ${ck.Sex}
                {{/if}}
            </td>
            <td>
                ${ck.jf}
            </td>
            <td>
                ${ck.jf_used}
            </td>
        </tr>
        {{/each}}
    </tbody>
</script>
<script>
    var dataUrl = "/j_C_Consumer_xfz/GetPage";
</script>