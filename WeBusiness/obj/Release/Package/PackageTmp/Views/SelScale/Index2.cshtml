@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="renderer" content="webkit|ie-comp|ie-stand" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" />
    <meta http-equiv="Cache-Control" content="no-siteapp" />
    <link rel="stylesheet" type="text/css" href="/layout/static/h-ui/css/H-ui.min.css" />
    <link rel="stylesheet" type="text/css" href="/layout/static/h-ui.admin/css/H-ui.admin.css" />
    <link rel="stylesheet" type="text/css" href="/layout/lib/Hui-iconfont/1.0.7/iconfont.css" />
    <link rel="stylesheet" type="text/css" href="/layout/lib/icheck/icheck.css" />
    <link rel="stylesheet" type="text/css" href="/layout/static/h-ui.admin/skin/default/skin.css"
          id="skin" />
    <link rel="stylesheet" type="text/css" href="/layout/static/h-ui.admin/css/style.css" />
    <title>防伪查询统计</title>
    <style type="text/css">
        .table td {
            text-align: center;
        }
    </style>
    <script src="~/js/common/Datehelper.js"></script>
    <script src="~/js/jquery-1.10.2.min.js"></script>
    <script src="~/js/jquery.tmpl.min.js"></script>

</head>
<body>

    <nav class="breadcrumb"><i class="Hui-iconfont">&#xe67f;</i> 首页 <span class="c-gray en">&gt;</span> 条码数据管理 <span class="c-gray en">&gt;</span> 窜货统计 <a class="btn btn-success radius r" style="line-height:1.6em;margin-top:3px" href="javascript:location.replace(location.href);" title="刷新"><i class="Hui-iconfont">&#xe68f;</i></a></nav>
    <div class="page-container">
        <form class="text-c" method="post" id="condition">
            <input name="orderby" type="hidden" value="" />
            <div class="text-c">
                查询地区：
                <input type="text" name="province" style="width:200px" placeholder="请输入地区关键字" class="input-text" />&nbsp;&nbsp;
                销售地区：
                <input type="text" name="Address" style="width:200px" placeholder="请输入地区关键字" class="input-text" />
                <button name="" id="search" class="btn btn-success" type="button">
                    <i class="Hui-iconfont">&#xe665;</i> 搜索
                </button>

            </div>
            <div class="mt-20">
                <table class="table table-border table-bordered table-hover table-bg dataTable" id="datahtml"></table>
                @Html.Partial("_Paging")
            </div>
        </form>
    </div>
    <script type="text/javascript" src="/layout/lib/layer/2.1/layer.js"></script>
    <script type="text/javascript" src="/layout/static/h-ui.admin/js/H-ui.admin.js"></script>
</body>
</html>
<script type="text/x-jquery-tmpl" id="datajs">
    <thead>
        <tr class="text-c">

            <th>
                查询地区
            </th>
            <th>
                销售地区
            </th>
            <th>
                状态
            </th>
            <th>
                窜货次数
            </th>
            @*<th>
                       详情
                </th>*@
        </tr>
    </thead>
    <tbody>
        {{each(i,ck) RtnList}}
        <tr class="text-c">

            <td>
                ${ck.province+ck.city}
            </td>
            <td>
                ${ck.Address}
            </td>
            <td>
                ${ck.warning}
            </td>
            <td>
                ${ck.cnt}
            </td>
            @*<td>
                    <a title="查看产品条码" href="javascript:;"
                       onclick="layer_show('查看窜货明细(查询地区:${ck.province+ck.city})', '/InStock/InSmallStockDetailIndex/${ck.BigCode}', '900', '500')" class="ml-5 edit" style="text-decoration: none">
                        <i class="Hui-iconfont">&#xe6f9;</i>
                    </a>
                </td>*@
        </tr>
        {{/each}}
    </tbody>
</script>
<script>
    var dataUrl = "/SelScale/GetCodePage2";


</script>
